{
  "name": "wlfpanda1012/easy-sts",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "hyperf"
  ],
  "description": "集成式Sts",
  "autoload": {
    "psr-4": {
      "Wlfpanda1012\\EasySts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HyperfTest\\": "tests"
    }
  },
  "require": {
    "php": ">=8.1",
    "hyperf/di": "3.1.*",
    "wlfpanda1012/common-sts": "^1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^1.0",
    "phpunit/phpunit": "^10.0",
    "swoole/ide-helper": "dev-master"
  },
  "suggest": {
    "swow/swow": "Required to create swow components."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit -c phpunit.xml --colors=always",
    "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
    "cs-fix": "php-cs-fixer fix $1"
  },
  "extra": {
    "hyperf": {
      "config": "Wlfpanda1012\\EasySts\\ConfigProvider"
    }
  }
}
